Key_Right = keyboard_check_direct(ord('D'));
Key_Jump = keyboard_check_direct(vk_space);
Key_Up = keyboard_check_direct(ord('W'));

if(Key_Left)
{
    hsp = -2;
    sprite_index = spr_walk_left;
    last_move = LEFT;
}

if(Key_Right)
{
    hsp = 2;
    sprite_index = spr_walk_right;
    last_move = RIGHT;
}

if((Key_Right &amp;&amp; Key_Left) or (!Key_Right &amp;&amp; !Key_Left))
{
    hsp = 0;
    if(last_move == RIGHT)
        sprite_index = spr_still_right;
    else if(last_move == LEFT)
        sprite_index = spr_still_left;
}

if(Key_Up &amp;&amp; can_enter) {
    room = rm_father;
    is_isometric_view = true;
}

if(Key_Jump)
{
    if(grounded) vsp = -7;
}

vsp += grav;

if (place_meeting (x,y+vsp, par_wall)) {
    while(!place_meeting(x, y+1, par_wall)) y += 1;
    vsp = 0;
    grounded = 1;
} else {
    grounded = 0;
}

if(place_meeting(x+hsp, y, par_wall))
{
    while (!place_meeting(x+sign(hsp), y, par_wall)) x += sign(hsp);
    hsp = 0;
}


x += hsp;
y += vsp;
