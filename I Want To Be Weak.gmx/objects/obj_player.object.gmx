Key_Bottom = keyboard_check_direct(ord('S'));
Key_Fire = keyboard_check_pressed(ord('F'));

direction_x = mouse_x - x;
direction_y = mouse_y - y;

length = sqrt(sqr(direction_x) + sqr(direction_y));

direction_x = direction_x / length;
direction_y = direction_y / length;

if(Key_Left || Key_Right || Key_Top || Key_Bottom) {

    if (abs(direction_x) &gt; abs(direction_y)) {
        if(direction_x &gt; 0) 
            player_sprite = spr_walk_right;
        else
            player_sprite = spr_walk_left;
    } else {
        if(direction_y &gt; 0) 
            player_sprite = spr_walk_front;
        else
            player_sprite = spr_walk_back;
    }
    
    if(Key_Left) {
        current_velocity_x = -velocity;
        if(!Key_Top &amp;&amp; !Key_Bottom)
            current_velocity_y = 0;   
    } else if (Key_Right) {
        current_velocity_x = velocity;
        if(!Key_Top &amp;&amp; !Key_Bottom)
            current_velocity_y = 0;  
    } 
    
    if (Key_Top) {
        if(!Key_Right &amp;&amp; !Key_Left)
            current_velocity_x = 0;
        current_velocity_y = -velocity;  
    } else if (Key_Bottom) {
        if(!Key_Right &amp;&amp; !Key_Left)
            current_velocity_x = 0;
        current_velocity_y = velocity;  
    } 
} else {
    if (abs(direction_x) &gt; abs(direction_y)) {
        if(direction_x &gt; 0) 
            player_sprite = spr_still_right;
        else
            player_sprite = spr_still_left;
    } else {
        if(direction_y &gt; 0) 
            player_sprite = spr_still_front;
        else
            player_sprite = spr_still_back;
    }
    
    current_velocity_x = 0;
    current_velocity_y = 0;  
}

if(mouse_check_button(mb_left) &amp;&amp; can_fireball) {
    fireball = instance_create(x,y, obj_fireball);
    fireball.hspeed = direction_x*fireball_speed;
    fireball.vspeed = direction_y*fireball_speed;
    can_fireball = false;
    alarm[0] = 20;
}
