            <string>Key_Left = keyboard_check_direct(ord('A'));
Key_Right = keyboard_check_direct(ord('D'));
Key_Top = keyboard_check_direct(ord('W'));
Key_Bottom = keyboard_check_direct(ord('S'));
Key_Fire = keyboard_check_pressed(ord('F'));

direction_x = mouse_x - x;
direction_y = mouse_y - y;

length = sqrt(sqr(direction_x) + sqr(direction_y));
if (length == 0)
    length = 0.00001;

direction_x = direction_x / length;
direction_y = direction_y / length;

if(Key_Left || Key_Right || Key_Top || Key_Bottom &amp;&amp; !is_checking_map) {

    if (abs(direction_x) &gt; abs(direction_y)) {
        if(direction_x &gt; 0) 
            player_sprite = spr_walk_right;
        else
            player_sprite = spr_walk_left;
    } else {
        if(direction_y &gt; 0) 
            player_sprite = spr_walk_front;
        else
            player_sprite = spr_walk_back;
    }
    
    if(Key_Left) {
        current_velocity_x = -velocity;
        if(!Key_Top &amp;&amp; !Key_Bottom)
            current_velocity_y = 0;   
    } else if (Key_Right) {
        current_velocity_x = velocity;
        if(!Key_Top &amp;&amp; !Key_Bottom)
            current_velocity_y = 0;  
    } 
    
    if (Key_Top) {
        if(!Key_Right &amp;&amp; !Key_Left)
            current_velocity_x = 0;
        current_velocity_y = -velocity;  
    } else if (Key_Bottom) {
        if(!Key_Right &amp;&amp; !Key_Left)
            current_velocity_x = 0;
        current_velocity_y = velocity;  
    } 
} else {
    if (abs(direction_x) &gt; abs(direction_y)) {
        if(direction_x &gt; 0) 
            player_sprite = spr_still_right;
        else
            player_sprite = spr_still_left;
    } else {
        if(direction_y &gt; 0) 
            player_sprite = spr_still_front;
        else
            player_sprite = spr_still_back;
    }
    
    current_velocity_x = 0;
    current_velocity_y = 0;  
}

if(stop_movement){
    current_velocity_x = 0;
    current_velocity_y = 0;
}

x += current_velocity_x;
y += current_velocity_y;

x = min(max(x, 0 + sprite_xoffset), room_width + sprite_xoffset - sprite_width);
y = min(max(y, 0 + sprite_yoffset), room_height + sprite_yoffset - sprite_height);

sprite_index = player_sprite;

if (room == rm_0)
    health = 100;

if (health &lt; 1) {
    show_message("That's not how it ends...");
    game_restart();
}

</string>
